echo '35  ls' >> history.txt
echo '36  cd ..' >> history.txt
echo '37  mv newfile.txt old.txt' >> history.txt
echo '38  mv old.txt training/' >> history.txt
echo '39  cd training' >> history.txt
echo '40  echo -e "#include<stdio.h>\nint main(){printf(\"Hello\\n\");return 0;}" > hello.c' >> history.txt
echo '41  gcc hello.c -o hello && ./hello' >> history.txt
echo '42  echo `date`' >> history.txt
echo '43  date' >> history.txt
echo '44  uname -r' >> history.txt
echo '45  echo "done"' >> history.txt

